                                </a>
                                <a href="/#!/event/[[ item.id ]]" class="row col-xs-12 margin-top_5 text-black text-bold text-underline_hover">
                                    [[ item.name ]]
                                </a>
                                <a href="/#!/event/[[ item.id ]]" class="row col-xs-12 margin-top_5 text-gray text-underline_none_hover" parse-description ng-model="item.description"></a>
                            </div>

                            <div ng-if="(key % 2) != 0" ng-class="{'container_image_events_big':((key % 2) != 0)}" class="block left">
                                <a href="/#!/events/[[ item.id_cat ]]" class="name_category">[[ item.name_cat ]]</a>

                                <a href="/#!/event/[[ item.id ]]" >
                                    <img ng-if="item.img" class="blur" ng-src="{{  path_img_event_original }}[[ item.path ]][[ item.img ]]" />
                                    <div ng-if="!item.img" class="glyphicon glyphicon-picture text-gray-light fs_3-2 default_img_event"></div>
                                    <div class="container_image_events_big_text col-xs-10 col-xs-offset-1">
                                        <div class="fs_2-0 text-underline">[[ item.name ]]</div>
                                        <div class="margin-top_30 col-xs-6">
                                            <span class="glyphicon glyphicon-eye-open text-white middle"></span>
                                            <span class="fs_0-7 text-white"> [[ item.viewed ]]</span>
                                            <span class="glyphicon glyphicon-comment text-white middle"></span>
                                            <span class="fs_0-7 text-white"> [[ item.count_comment ]]</span>

