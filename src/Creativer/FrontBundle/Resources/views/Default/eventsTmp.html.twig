                                    <div class="col-xs-6 padding-right_5 text-right fs_0-8 text-gray-light" ng-if="item.start_date != item.end_date">
                                        <time>[[ item.start_date | date: "dd.MM.yyyy" ]]</time><br>
                                        <time>[[ item.end_date | date: "dd.MM.yyyy" ]]</time>
                                    </div>
                                    <div class="col-xs-6 padding-right_5 text-right fs_0-8 text-gray-light" ng-if="item.start_date == item.end_date">
                                        <time>[[ item.start_date | date: "dd.MM.yyyy" ]]</time><br>
