                                        <div class="col-xs-6 padding-right_5 text-right fs_0-8 text-white" ng-if="item.start_date == item.end_date">
                                            <time>[[ item.start_date | date: "dd.MM.yyyy" ]]</time><br>
                                        </div>
                                    </div>
                                </a>
                            </div>

